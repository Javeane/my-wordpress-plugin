/**
 * Shortcode to display a random quote
 */
function my_wp_plugin_random_quote_shortcode() {
    $args =3D array(
        'post_type'      =3D&gt; 'my_wp_plugin_quotes',
        'posts_per_page' =3D&gt; 1,
        'orderby'        =3D&gt; 'rand',
    );
    $query =3D new WP_Query( $args );

    if ( $query-&gt;have_posts() ) {
        while ( $query-&gt;have_posts() ) {
            $query-&gt;the_post();
            $output =3D '&lt;div class=3D"my-wp-plugin-random-quote"&gt;';
            $output .=3D '&lt;blockquote&gt;' . get_the_content() . '&lt;/b=
lockquote&gt;';
            $output .=3D '&lt;cite&gt;' . get_the_title() . '&lt;/cite&gt;'=
;
            $output .=3D '&lt;/div&gt;';
        }
        wp_reset_postdata();
        return $output;
    }
}
