/**
 * Handle social login requests.
 *
 * @param array $args Login arguments.
 */
public function social_login( $args ) {
	$social_login_provider =3D isset( $_REQUEST['provider'] ) ? sanitize_text_=
field( wp_unslash( $_REQUEST['provider'] ) ) : '';

	if ( ! $this-&gt;is_valid_provider( $social_login_provider ) ) {
		return;
	}

	$this-&gt;set_provider_session( $social_login_provider );
	wp_safe_redirect( wp_login_url() );
	exit;
}
